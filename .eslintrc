{
    "rules": {
        "quotes": [2, "single"],
        "no-underscore-dangle": 0,
        "dot-notation": 0,
        "curly": [2, "multi-line"],

        // Strict Mode
        "strict": [2, "global"],

        // Variables
        "no-catch-shadow": 2,
        "no-delete-var": 2,
        "no-label-var": 2,
        "no-shadow": 2,
        "no-shadow-restricted-names": 2,
        "no-undef": 2,
        "no-undef-init": 2,
        "no-undefined": 2,
        "no-unused-vars": 2,
        "no-use-before-define": 2
    },
    "env": {
        "browser": true,
        "node": true
    },
    "ecmaFeatures": {
        "jsx": true
    },
    "plugins": [
        "react"
    ]
}