{
    "rules": {
        "quotes": [2, "single"],
        "no-undef": 1,
        "no-unused-expressions": 1,
        "no-unused-vars": 1,
        "strict": 1,
        "no-underscore-dangle": 0
    },
    "env": {
        "browser": true,
        "node": true,
    },
    "globals": {
        "BROWSER": true
    }
}